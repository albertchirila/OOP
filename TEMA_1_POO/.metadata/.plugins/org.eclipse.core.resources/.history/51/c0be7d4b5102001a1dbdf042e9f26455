
/** @author Chirila Albert */
public class Beneficii {
	/** valoare int specifica imbarcarii prioritare */
	private int points1;
	/** valoare int specifica nevoilor speciale */
	private int points2;
	
	/**
	 * @param priority_embark imbarcare prioritara
	 * @param special nevoi speciale
	 */
	public Beneficii(boolean priority_embark, boolean special) {
		if(priority_embark == true)
			this.points1 = 30;
		else
			this.points1 = 0;
		
		if(special == true)
			this.points2 = 100;
		else
			this.points2 = 0;
	}
	
	/** getter ce returneaza punctele specifice imbarcarii prioritare */
	public int getPoints1() {
		return this.points1;
	}
	
	/** getter ce returneaza punctele specifice nevoilor speciale */
	public int getPoints2() {
		return this.points2;
	}
}
